{
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module", 
        "ecmaFeatures":{
            "jsx": true
        }
    },
    "env":{
        "browser": true,
        "node": true,
        "es6":true
    },
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "import",
        "react-hooks"
    ],
    "rules":{
        "no-underscore-dangle":"off",
        "react/forbid-prop-types": 0,
        "react/jsx-filename-extension": 0,
        "indent": 0,
        "space-before-blocks": 0
    }
}